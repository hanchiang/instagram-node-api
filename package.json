{
  "name": "instagram-node-api",
  "version": "1.0.0",
  "description": "A node.js instagram API",
  "main": "index.js",
  "scripts": {
    "dev": "./node_modules/.bin/nodemon --inspect=0.0.0.0:9229 ./bin/www | ./node_modules/.bin/bunyan",
    "test": "NODE_ENV=test mocha 'tests/**/*.js' --timeout 10000",
    "test-watch": "nodemon --exec \"npm test\"",
    "lint": "./node_modules/.bin/eslint \"**/*.js\"",
    "create-table": "node ./db/scripts/create-table.js 1000",
    "compose-test-up": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up -d",
    "compose-test-down": "docker-compose -f docker-compose.yml -f docker-compose.test.yml down"
  },
  "author": "Han Chiang",
  "keywords": [
    "Instagram",
    "API",
    "node"
  ],
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-express-serializer": "^1.0.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^1.30.1",
    "express": "4.16.4",
    "express-request-id": "^1.4.1",
    "helmet": "^3.15.0",
    "htmlparser2": "^3.10.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "numeral": "^2.0.6",
    "pg": "^7.8.0",
    "request-ip": "^2.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "sinon": "^7.2.3",
    "supertest": "^3.4.2"
  }
}
