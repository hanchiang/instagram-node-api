{
  "name": "instagram-node-api",
  "version": "1.0.0",
  "description": "A node.js instagram API",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test npm run test-unit && npm run test-integration",
    "test-integration": "NODE_ENV=test nyc mocha -r ts-node/register test/integration/index.ts",
    "test-unit": "NODE_ENV=test nyc mocha -r ts-node/register test/unit/index.ts",
    "build": "npm run remove-dist && npm run build-ts && npm run copy-static-assets && npm run lint && npm run format",
    "remove-dist": "rm -rf dist",
    "copy-static-assets": "node copy-static-assets.js",
    "format": "prettier --write \"**/*.+(ts|js|json)\"",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "compose-test-up": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up -d",
    "compose-test-down": "docker-compose -f docker-compose.yml -f docker-compose.test.yml down",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/bin/www.js | ./node_modules/.bin/bunyan",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "author": "Han Chiang",
  "keywords": [
    "Instagram",
    "API",
    "node"
  ],
  "license": "ISC",
  "dependencies": {
    "apisauce": "^1.1.2",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-express-serializer": "^1.0.0",
    "cls-hooked": "^4.2.2",
    "csv-writer": "^1.6.0",
    "express": "4.16.4",
    "express-request-id": "^1.4.1",
    "helmet": "^4.0.0",
    "htmlparser2": "^3.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "numeral": "^2.0.6",
    "request-ip": "^2.1.3",
    "sequelize": "^6.3.3",
    "tweetnacl-sealedbox-js": "^1.2.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/body-parser": "^1.17.1",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.12",
    "@types/cls-hooked": "^4.3.1",
    "@types/concurrently": "^5.2.1",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^6.8.1",
    "@types/express": "^4.17.7",
    "@types/express-request-id": "^1.4.1",
    "@types/helmet": "0.0.47",
    "@types/htmlparser2": "^3.10.1",
    "@types/lodash": "^4.14.158",
    "@types/mocha": "^7.0.2",
    "@types/morgan": "^1.9.1",
    "@types/node": "^12.12.53",
    "@types/numeral": "0.0.28",
    "@types/request-ip": "0.0.35",
    "@types/shelljs": "^0.8.8",
    "@types/sinon": "^9.0.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "chai": "^4.2.0",
    "concurrently": "^5.0.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "shelljs": "^0.8.4",
    "sinon": "^7.2.3",
    "supertest": "^3.4.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
